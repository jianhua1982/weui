<div class="bd">

    <!--bottom tabbar-->

    <div class="weui_tabbar">
        <a href="javascript:;" class="weui_tabbar_item discover">
            <div class="weui_tabbar_icon">
                <img src="https://weui.github.io/weui/images/icon_nav_button.png" alt="">
            </div>
            <p class="weui_tabbar_label">发现</p>
        </a>

        <a href="javascript:;" class="weui_tabbar_item mine">
            <div class="weui_tabbar_icon">
                <img src="https://weui.github.io/weui/images/icon_nav_msg.png" alt="">
            </div>
            <p class="weui_tabbar_label">我</p>
        </a>

        <a href="javascript:;" class="weui_tabbar_item upload_images">
            <div class="weui_tabbar_icon">
                <img src="http://weui.github.io/weui/images/icon_nav_search_bar.png" alt="">
            </div>
            <p class="weui_tabbar_label">我要上传</p>
        </a>
    </div>

    <!--discover section tab -->

    <section class="discover">

        <!-- Swiper -->
        <div class="swiper-container">
            <div class="swiper-wrapper">
                <!--image path relative to index.html-->
                <div class="swiper-slide" style="background-image:url('./images/sns/f1.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f2.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f3.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f4.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f5.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f6.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f7.jpg')"></div>
                <div class="swiper-slide" style="background-image:url('./images/sns/f8.jpg')"></div>
            </div>

            <!-- Add Pagination -->
            <div class="swiper-pagination swiper-pagination-bullets"></div>
        </div>


        <div style="display: none;">
            <ul class="grid_images">
                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f1.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f2.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f3.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f4.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f5.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f6.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f7.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f8.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/more.png">
                    </a>
                </li>

                <!--sp-->

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f1.jpg">
                    </a>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <img src="./images/sns/f2.jpg">
                    </a>
                </li>
            </ul>
        </div>

        <div>
            <div class="msg_timeline">
                <script id="timeline-template" type="text/x-handlebars-template">

                    <ul class="timeline">
                        {{#persons}}
                        <li class="">
                            <div class="user_info weui_media_box weui_media_appmsg">
                                <div class="weui_media_hd">
                                    <img class="weui_media_appmsg_thumb" src="{{headimgurl}}" alt="">
                                </div>

                                <div class="weui_media_bd">
                                    <p class="weui_media_title">{{nickname}}</p>
                                    <p class="weui_media_desc">{{emotion}}</p>

                                    <div class="wx_selected_images">
                                        <script id="timeline-images-template" type="text/x-handlebars-template">

                                            <ul class="grid_images">
                                                {{#images}}
                                                <li class="">
                                                    <a href="javascript:;">
                                                        <img src="{{localId}}">
                                                    </a>
                                                </li>
                                                {{/images}}
                                            </ul>
                                        </script>
                                    </div> 

                                </div>
                            </div>
                        </li>
                        {{/persons}}
                    </ul>
                </script>
            </div>
        </div>
    </section>

    <!--mine section tab -->

    <section class="mine" style="display: none">

        <div class="user_info weui_media_box weui_media_appmsg">
            <div class="weui_media_hd">
                <img class="weui_media_appmsg_thumb" src="data:image/png;base64," alt="">
            </div>

            <div class="weui_media_bd">
                <h4 class="weui_media_title">喻建华</h4>
                <p class="weui_media_desc">用户个人资料填写的省份</p>
            </div>
        </div>

        <ul>
            <li>
                <p> </p>
            </li>
        </ul>
    </section>


    <!--upload image tab -->

    <section class="upload_images" style="display: none">
        <div class="upload_page" style="display: none">
            <textarea class="weui_textarea" placeholder="这一刻的想法">  </textarea>

            <div class="wx_selected_images">
                <script id="wx-selected-images-template" type="text/x-handlebars-template">

                    <ul class="grid_images">
                        {{#localIds}}
                        <li class="">
                            <a href="javascript:;">
                                <img src="{{localId}}">
                            </a>
                        </li>
                        {{/localIds}}
                    </ul>
                </script>
            </div> 

            <div class="bd spacing">
                <a href="javascript:;" class="weui_btn weui_btn_primary">上传美图</a>
                <!--<a href="javascript:;" class="weui_btn weui_btn_disabled weui_btn_primary">按钮</a>-->
            </div>

        </div>
    </section>

</div>
